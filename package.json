{
  "name": "eb2-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "prepwa": "ng build",
    "pwa": "npx http-server -p 8080 -c-1 dist/eb2-ui/browser",
    "test": "ng test",
    "generate": "graphql-codegen"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^21.0.5",
    "@angular/common": "^21.1.1",
    "@angular/compiler": "^21.1.1",
    "@angular/core": "^21.1.1",
    "@angular/forms": "^21.1.1",
    "@angular/material": "^21.0.5",
    "@angular/platform-browser": "^21.1.1",
    "@angular/router": "^21.1.1",
    "@angular/service-worker": "^21.1.1",
    "@apollo/client": "^4.0.11",
    "angularx-qrcode": "^21.0.4",
    "apollo-angular": "^13.0.0",
    "chart.js": "^4.3.0",
    "chartjs-plugin-datalabels": "^2.2.0",
    "graphql": "^16.12.0",
    "graphql-sse": "^2.6.0",
    "luxon": "^3.7.2",
    "ng2-charts": "^8.0.0",
    "ngx-infinite-scroll": "^21.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.1",
    "@angular/cli": "^21.1.1",
    "@angular/compiler-cli": "^21.1.1",
    "@graphql-codegen/cli": "^6.1.0",
    "@graphql-codegen/typescript": "^5.0.7",
    "@graphql-codegen/typescript-apollo-angular": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^5.0.7",
    "@types/luxon": "^3.7.1",
    "baseline-browser-mapping": "^2.9.11",
    "jsdom": "^27.4.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
